<h4 class="center-align">Usuarios mas populares</h4>
<div class="col s12">
	<div class="col s4 offset-s4">
		<div class="center-align">
			<%if(users[0].picture!=""){%>
			<img class="responsive-img materialboxed" id="pic" src="{{APP_IMAGE_PATH}}<%=users[0].picture%>">
			<%}else{%>
			<img src="{{APP_RESOURCES}}/image/user.jpg">
			<%}%>
			<a onclick="apretaste.send({'command':'PIZARRA PERFIL','data':{'username':'<%=users[0].username%>'}})"><%=users[0].username%></a>
			<br><b><%=users[0].reputation%></b>
		</div>
	</div>
</div>

