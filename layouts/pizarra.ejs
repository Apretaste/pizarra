<%
var menu = [
    {link: '', caption: 'Muro'},
    {link: 'GLOBAL', caption: 'Global'},
    {link: 'POPULARES', caption: 'Tendencia'},
    {link: 'INFLUENCERS', caption: 'Influencers'}
];
%>

<!-- TABS -->
<div class="row" style="margin-bottom: 0.75rem">
    <div class="col s12">
        <ul class="tabs tabs-fixed-width">
            <% menu.forEach(function(e) { %>
                <% var active = typeof title != "undefined" && title == e.caption ? 'active' : '' %>
                <li class="tab">
                    <a href="#" class="green-text <%= active %>"
                       onclick="apretaste.send({'command':'PIZARRA <%= e.link %>'})"
                    >
                        <%= e.caption %>
                    </a>
                </li>
            <% }); %>
        </ul>
    </div>
</div>

<!-- CONTENT -->
<div class="row">
    {{APP_TEMPLATE_CODE}}
</div>

<%- '<serviceImgPath data="{{APP_SERVICE_PATH}}/images/"></serviceImgPath>' %>
